if($('<%=".website-#{@website.id} .bookmarks-list"%>').length){ 
	$('<%=".website-#{@website.id} .bookmarks-list"%>').slideUp(700, function(){ 
		$(this).remove(); 
	}); 
}else{ 
	$('<%= ".website-#{@website.id}" %>').append($("<%=j render partial: "website_bookmarks", locals: {website: @website}%>").hide().delay(40).slideDown(700)); 
}
