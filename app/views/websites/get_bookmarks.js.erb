if($('<%=".website-#{@website.id} .bookmarks-list"%>').length){ 
	$('<%=".website-#{@website.id} .bookmarks-list"%>').slideUp(700, function(){ 
		$(this).remove(); 
	}); 
}else{
	<% if @website.bookmarks.present? %> 
		$('<%= ".website-#{@website.id}" %>').append($("<%=j render partial: "website_bookmarks", locals: {website: @website}%>").hide().delay(20).slideDown(700));
	<% else %>
		alert('<%= "#{@website.url} does not have any bookmarks" %>')
	<% end %>	 
}
